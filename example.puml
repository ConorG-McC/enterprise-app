@startuml
class User {
  +String name
  +String email
  +void login()
  +void logout()
}
 
class Product {
  +String name
  +float price
  +void displayDetails()
}
 
class Order {
  +int orderId
  +Date date
  +addItem(Product p)
  +removeItem(Product p)
}
 
User --> Order : places >
Order *--> Product : contains >
@enduml